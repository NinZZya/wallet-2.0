{
  "name": "wallet",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npx env-cmd -f .env npx webpack-dev-server --mode=development --open --hot",
    "build": "npx env-cmd -f .env.production npx webpack --mode=production",
    "build:firebase": "npx env-cmd -f .env webpack --mode=production",
    "lint": "npx eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "loki:update": "npm run build-storybook && npx loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "npx loki approve",
    "test": "npx jest",
    "test:loki": "npm run build-storybook && npx loki --requireReference --reactUri file:./storybook-static",
    "test:stat": "npx jest --coverage",
    "check": "npm run lint && npm test && npm run test:loki",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "predeploy:firebase": "rm -rf dist && mkdir dist && npm run build:firebase && cp -R src/coverage/lcov-report dist/coverage/ && cp -R storybook-static dist/storybook/",
    "deploy:firebase": "npm run predeploy:firebase && npx firebase deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azarov-serge/wallet-2.0"
